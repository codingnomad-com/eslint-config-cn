{
  "name": "@codingnomad/eslint-config-cn",
  "version": "0.1.17",
  "main": "index.js",
  "repository": "github:codingnomad-com/eslint-config-cn",
  "author": "Manuel Bachl | Coding Nomad <manuel@coding-nomad.com> (https://coding-nomad.com)",
  "homepage": "https://coding-nomad.com",
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "release": "standard-version",
    "release:tags": "git push --follow-tags origin main",
    "prepublishOnly": "yarn release && yarn release:tags"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "eslint": "8.37.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "29.5.0",
    "prettier": "2.8.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "commitizen": "4.3.0",
    "read-pkg-up": "9.1.0",
    "standard-version": "9.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "eslint",
    "prettier",
    "stylelint",
    "commitlint"
  ]
}
